{
  "name": "huppys/cookie-consent-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle to ask the visitors for their consent to use cookies to comply to AVG/GDPR.",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "symfony/asset": "^7.0",
    "symfony/browser-kit": "^7.0",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/doctrine-bridge": "^7.0",
    "symfony/expression-language": "^7.0",
    "symfony/form": "^7.0",
    "symfony/http-foundation": "^7.1",
    "symfony/http-kernel": "^7.0",
    "symfony/orm-pack": "^2.4",
    "symfony/security-csrf": "^7.0",
    "symfony/translation": "^7.0",
    "symfony/twig-bridge": "^7.0",
    "symfony/twig-bundle": "^7.0",
    "twig/twig": "3.14.0.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v3.38.2",
    "phpunit/phpunit": "^10.4.2 || ^11.1.2",
    "symfony/maker-bundle": "^1.61",
    "symfony/runtime": "^7.1",
    "symfony/test-pack": "^1.0",
    "symfony/yaml": "^6.0 || ^7.0"
  },
  "prefer-stable": true,
  "autoload": {
    "exclude-from-classmap": [
      "/tests/",
      "/node_modules/"
    ],
    "psr-4": {
      "huppys\\CookieConsentBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "huppys\\CookieConsentBundle\\tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
